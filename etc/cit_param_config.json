            "caption":"电池测试",
            "config":{
                "support_dual_connector":false,
                "battery_dual_connector_path_config":"/sys/class/power_supply/bq2597x-master/ti_battery_present",
                "battery_id_path_config":"/sys/class/mtk-battery/battery/battery_id_voltage"
            }
        },
        {
            "name":"home_sensor_sar_data_collect_test",
            "caption":"SAR Sensor数据采集测试",
            "config":{
                "first_sar_chanel_num": 9,
                "second_sar_chanel_num": 9,
                "c0_threshold_far_first_sar":[0,1999],
                "c0_threshold_near_first_sar":[2000,30000],
                "c3_threshold_far_second_sar":[0,1999],
                "c3_threshold_near_second_sar":[2000,30000]
            }

        },
        {
            "name": "home_fp_sensor_check_test",
            "caption": "指纹传感器测试"
        },
        {
            "name":"home_fp_check_test",
            "caption":"指纹录入测试"
        }

