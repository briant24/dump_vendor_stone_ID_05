                    {
                        "name": "cit_lcdlight_brightness",
                        "type": "screen_brightness",
                        "max_brightness":2047,
                        "breath":true
                    },
                    {
                        "name": "cit_torch_first",
                        "type": "general_camera_flash",
                        "torch_node_path": "/sys/devices/platform/flashlights_led191/led191_FLASH",
