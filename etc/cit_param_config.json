            "config":{
                "camera_id_front_main":1
            }
        },
        {
            "name":"home_rear_dual_camera_test",
            "caption":"后置双摄相机测试",
            "config":{
                "camera_id_rear_dual":61
            }
        },
        {
            "name":"home_nfc_test",
            "caption":"NFC测试",
            "config":{
                "eseNFC_chip_model":"pn100t"
            }
        },
        {
            "name":"home_sensor_gps_test",
            "caption":"GPS测试"
        },
        {
